@page
@model AKPWB240601.Pages.WrittenExam.GenerateCallLetter.ViewCallLetterModel
@{
    Layout = "_Post";
}
<section class="main-section">
    <div class="header-below d-flex align-items-center justify-content-between py-2 shadow-sm bg-white ">
        <p class="mb-0" id="home-p">Recruitment of Stenographer and Data Entry Assistant for Scheduled Caste & Scheduled Tribes - 02 Post</p>
        <p class="mb-0"><img class="header-below-logo" src="~/images/LOGO.png"> KSP(Karnataka State Police) Recruitment Cell, Bengaluru, Karnataka.</p>
    </div>
    <!-- Start Breadcumb -->
    <section id="main" class="main">
        <section id="breadcrumb">
            <div class="container-fluid my-1">
                <div class="row">
                    <div class="col-12">
                        <div class="d-flex flex-wrap justify-content-between align-items-center">
                            <div class="d-flex align-items-center">
                                <a href="/WrittenExam/GenerateCallLetter/GenerateCallLetter" class="text-decoration-none text-dark mt-2">
                                    <span class="material-symbols-outlined me-2">arrow_back</span>
                                </a>
                                <h5 class="page-title mb-0">View details</h5>
                            </div>


                            <ul class="breadcrumb mb-0">
                                <li class="breadcrumb-item">
                                    <a href="/Posts" class="text-decoration-none">
                                        Post
                                    </a>
                                </li>
                                <li>
                                    <span class="material-symbols-outlined">
                                        chevron_right
                                    </span>Written exams
                                </li>
                                <li>
                                    <span class="material-symbols-outlined">
                                        chevron_right
                                    </span>Generate Call Letter
                                </li>
                                <li class="active">
                                    <span class="material-symbols-outlined">
                                        chevron_right
                                    </span>View Details
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </section>
    <!-- End Breadcumb -->
    <div class="Preview-container p-3">
        <div class="previews">
            <h5 class="p-2">Generate Call Letter details</h5>
        </div>
        <div class="preview-content">
            <div class="preview">
                <table>
                
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Unit Name<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Unit Name" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">College Name<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the College Name" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Order No<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Order No" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Direct Serving<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Direct Serving" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Candidate Name<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Candidate Name" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Date of birth <span>:</span></td>
                        <td class="addpost-td border-box position-relative">
                            <input type="text" placeholder="dd/mm/yyyy" class="add-post-input1 pl-4">
                            <span class="material-symbols-outlined " style="right:368px;top:20px;position:absolute">
                                calendar_month
                            </span>
                        </td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Gender<span>:</span></td>
                        <td class="addpost-td border-box">
                            <select class="add-post-input1 form-select">
                                <option value="" disabled selected>Select the Gender</option>
                                <option value="option1">Option 1</option>
                                <option value="option2">Option 2</option>
                                <option value="option3">Option 3</option>
                            </select>
                        </td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Reservation<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Reservation" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Roll No<span>:</span></td>
                        <td class="addpost-td border-box"><input type="text" placeholder="Enter the Roll no" class="add-post-input1"></td>
                    </tr>
                    <tr>
                        <td class="addpost-th d-flex justify-content-between">Date & Time <span>:</span></td>
                        <td class="addpost-td border-box position-relative">
                            <input type="text" placeholder="dd/mm/yyyy" class="add-post-input1 pl-4">
                            <span class="material-symbols-outlined " style="right:368px;top:20px;position:absolute">
                                calendar_month
                            </span>
                            <input type="text" placeholder="dd/mm/yyyy" class="add-post-input1 pl-4">
                            <span class="material-symbols-outlined " style="right:10px;top:20px;position:absolute">
                                calendar_month
                            </span>
                        </td>
                    </tr>


                </table>
            </div>
        </div>



    </div>
</section>
