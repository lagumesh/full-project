@page
@model UAMS3.Admin.Pages.studycentresModel
@{
    Layout = "_Layout";
}

<!-- Start Breadcumb -->
<section id="main" class="main">
	<section id="breadcrumb">
		<div class="container-fluid my-2">
			<div class="row">
				<div class="col-12">
					<div class="d-flex flex-wrap justify-content-between">
						<div>
							<p class="page-title mb-0">Study Centre</p>
						</div>
						<ol class="breadcrumb mb-0">
							<i class="bi bi-house-door-fill"></i>
							<li class="breadcrumb-item">
								Dashboard <span><i class="bi bi-chevron-right"></i></span>
							</li>
							<li class="active">Study Centre</li>
						</ol>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End Breadcumb -->

	<!-- Start Study Centre Section -->
	<section class="study-centre mb-5">
		<div class="container-fluid">
			<div class="row">
				<div class="col-12">
					<!-- Start Add Delete Section -->
					<section class="delete-add-search mb-3">
						<div class="card">
							<div class="card-body py-2">
								<div class="d-flex justify-content-between align-items-center">
									<div>
										<button class="delete-btn"><span><i class="bi bi-trash3"></i></span>Delete</button>
										<select class="select me-2 status-dropdown" aria-label="Default select example">
											<option defaultValue>Status</option>
											<option>Enabled</option>
											<option>Disabled</option>
										</select>
									</div>
									<a href="/addstudycentres">
										<button class="add-btn"><span><i class="bi bi-plus-lg"></i></span>Add</button>
									</a>
								</div>
							</div>
						</div>
					</section>
					<!-- End  Add Delete Section -->

					<!-- Start Study Centre Table Section -->
					<div class="table-responsive">
						<table id="example" class="table table-striped table-hover" style="width: 100%">
							<thead>
								<tr>
									<th></th>
									<th>Code</th>
									<th>D Code</th>
									<th>Full Name</th>
									<th>City</th>
									<th>State</th>
									<th>Pin Code</th>
									<th>Mobile No</th>
									<th>College Info</th>
									<th>University</th>
									<th>Status</th>
								</tr>
							</thead>

							<tbody>
								<tr>
									<td></td>
									<td>5678</td>
									<td>4567</td>
									<td>Charles David</td>
									<td>Banglore</td>
									<td>Karnataka</td>
									<td>587654</td>
									<td>+91-99999 99999</td>
									<td>Yes</td>
									<td>Madhurai Meenakshi Degree College</td>
									<td>Enabled</td>
								</tr>
							</tbody>
						</table>
					</div>
					<!-- End Study Centre Table Section -->
				</div>
			</div>
		</div>
	</section>
</section>
<!-- End Study Centre Section -->