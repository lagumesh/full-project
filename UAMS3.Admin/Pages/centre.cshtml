@page
@model UAMS3.Admin.Pages.centreModel
@{
	Layout = "_Layout";
}

<!-- Start Breadcumb -->
<section id="main" class="main">
	<section id="breadcrumb">
		<div class="container-fluid my-2">
			<div class="row">
				<div class="col-12">
					<div class="d-flex flex-wrap justify-content-between">
						<div>
							<p class="page-title mb-0">Centre</p>
						</div>
						<ol class="breadcrumb mb-0">
							<i class="bi bi-house-door-fill"></i>
							<li class="breadcrumb-item">
								Dashboard <span><i class="bi bi-chevron-right"></i></span>
							</li>
							<li class="active">Centre</li>
						</ol>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End Breadcumb -->
	<!-- Start Centre Section -->
	<section class="centre mb-5">
		<div class="container-fluid">
			<div class="row">
				<div class="col-12">
					<!-- Start Add Delete Section -->
					<section class="delete-add-search mb-3">
						<div class="card">
							<div class="card-body py-2">
								<div class="d-flex justify-content-between align-items-center">
									<div>
										<button class="delete-btn"><span><i class="bi bi-trash3"></i></span>Delete</button>
										<select class="select me-2 status-dropdown" aria-label="Default select example">
											<option defaultValue>Status</option>
											<option>Enabled</option>
											<option>Disabled</option>
										</select>
									</div>
									<div>
										<a href="/fileuploads">
											<button class="import-btn">
												Import <span><i class="bi bi-plus-lg"></i></span>
											</button>
										</a>
										@* <input type="file" id="file-input" style="display: none;" accept=".xls, .xlsx">	 *@				
										<a href="/addcentres">
											<button class="add-btn"><span><i class="bi bi-plus-lg"></i></span>Add</button>
										</a>
									</div>
								</div>
							</div>
						</div>
					</section>
					<!-- End  Add Delete Section -->
					<!-- Start Centre Table Section -->
					<div class="table-responsive">
						<table id="example" class="table table-striped table-hover" style="width: 100%">
							<thead>
								<tr>
									<th><input type="checkbox" id="select-all"></th>
									<th>Code</th>
									<th>Name</th>
									<th>Taluk</th>
									<th>City</th>
									<th>District</th>
									<th>State</th>
									<th>Pin</th>
									<th>Phone</th>
									<th>Email</th>
									<th>University</th>
									<th>Status</th>
									<th>Actions</th>
								</tr>
							</thead>

							<tbody>
								<tr>
									<td type="checkbox"></td>
									<td>989898765</td>
									<td>Adolph Hitler</td>
									<td>Annekal</td>
									<td>Ballari</td>
									<td>Mysuru</td>
									<td>Karnataka</td>
									<td>587315</td>
									<td>9999999999</td>
									<td>exam@gmail.com</td>
									<td>University Name 01</td>
									<td>Enabled</td>
									<td class="icon">
										<i class="bi bi-pen"></i>
										<i class="bi bi-eye"></i>
									</td>
								</tr>
							</tbody>
						</table>
					</div>
					<!-- End Centre Table Section -->
				</div>
			</div>
		</div>
	</section>
</section>
<!-- End Centre Section -->


