@page
@model UAMS3.Admin.Pages.districtModel
@{
    Layout = "_Layout";
}

<!-- Start Breadcumb -->
<section id="main" class="main">
	<section id="breadcrumb">
		<div class="container-fluid my-2">
			<div class="row">
				<div class="col-12">
					<div class="d-flex flex-wrap justify-content-between">
						<div>
							<p class="page-title mb-0">District</p>
						</div>
						<ol class="breadcrumb mb-0">
							<i class="bi bi-house-door-fill"></i>
							<li class="breadcrumb-item">
								Dashboard <span><i class="bi bi-chevron-right"></i></span>
							</li>
							<li class="active">District</li>
						</ol>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End Breadcumb -->

	<!-- Start District Section -->
	<section class="districts mb-5">
		<div class="container-fluid">
			<div class="row">
				<div class="col-12">
					<!-- Start Add Delete Section -->
					<section class="delete-add-search mb-3">
						<div class="card">
							<div class="card-body py-2">
								<div class="d-flex justify-content-between align-items-center">
									<button class="delete-btn"><span><i class="bi bi-trash3"></i></span>Delete</button>
									<a href="/adddistricts">
										<button class="add-btn"><span><i class="bi bi-plus-lg"></i></span>Add</button>
									</a>
								</div>
							</div>
						</div>
					</section>
					<!-- End Add Delete Section -->

					<!-- Start District Table Section -->
					<div class="table-responsive">
						<table id="example" class="table table-striped table-hover" style="width: 100%">
							<thead>
								<tr>
									<th></th>
									<th>Code</th>
									<th>Display Title</th>
									<th>Union State</th>
								</tr>
							</thead>

							<tbody>
								<tr>
									<td></td>
									<td>98765</td>
									<td>Chennai</td>
									<td>TamilNadu</td>
								</tr>
							</tbody>
						</table>
					</div>
					<!-- End District Table Section -->
				</div>
			</div>
		</div>
	</section>
</section>
<!-- End District Section -->